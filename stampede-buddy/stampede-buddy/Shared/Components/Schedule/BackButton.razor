@inject AppState AppState

<div class="title-back my-3">
    <img class="icon-s" src="https://cdn-icons-png.flaticon.com/512/3114/3114883.png" @onclick="PreviousScreen" style="cursor: pointer;"/>
    <h2 class="mt-1">@Title</h2>
</div>

@code {
    [Parameter] public string? Title { get; set; }

    public void PreviousScreen()
    {
        AppState.GoToPreviousScreen();
    }

}

